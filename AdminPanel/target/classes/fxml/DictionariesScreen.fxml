<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>

<StackPane fx:id="dictionariesStackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd2.adminPanel.controllers.DictionariesController">
   <children>
      <AnchorPane>
         <children>
            <BorderPane prefHeight="408.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <top>
                  <StackPane prefHeight="45.0" prefWidth="200.0" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Zarządzanie słownikami" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </StackPane>
               </top>
               <bottom>
                  <StackPane prefHeight="45.0" prefWidth="200.0" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="buttonBackMenu" cancelButton="true" mnemonicParsing="false" onAction="#backMenu" stylesheets="@GreenButton.css" text="Wróć">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Button>
                     </children>
                  </StackPane>
               </bottom>
               <center>
                  <VBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Button fx:id="buttonDaysOfWeek" mnemonicParsing="false" onAction="#daysOfWeek" prefHeight="50.0" prefWidth="130.0" stylesheets="@GreenButtonBig.css" text="Dni tygodnia" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="buttonGroupsOfClients" mnemonicParsing="false" onAction="#GroupsOfClients" prefHeight="50.0" prefWidth="130.0" stylesheets="@GreenButtonBig.css" text="Grupy klientów">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="buttonEventTypes" mnemonicParsing="false" onAction="#EventTypes" prefHeight="50.0" prefWidth="130.0" stylesheets="@GreenButtonBig.css" text="Typy wydarzeń">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="buttonBuildings" mnemonicParsing="false" onAction="#Buildings" prefHeight="50.0" prefWidth="130.0" stylesheets="@GreenButtonBig.css" text="Budynki oraz pomieszczenia" textAlignment="CENTER" wrapText="true">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
               </center>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</StackPane>
